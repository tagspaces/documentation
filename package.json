{
  "name": "tagspaces",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start --port 3001",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "serve": "docusaurus serve",
    "gen-pdf-bak": "npx docusaurus-pdf from-build-config",
    "gen-pdf": "npx docs-to-pdf --initialDocURLs='http://localhost:3000/' --contentSelector='article' --paginationSelector='a.pagination-nav__link.pagination-nav__link--next' --excludeSelectors='.margin-vert--xl a,[class^='tocCollapsible'],.breadcrumbs,.theme-edit-this-page' --coverImage='http://localhost:3000/img/logo.svg' --coverTitle='TagSpaces v6 - User Documentation'",
    "update-ext": "./update-external.sh",
    "anigif": "gifify --colors 150 --to 19 media/input.webm -o media/result.gif",
    "prepare1": "yarn optipng && yarn update-ext && yarn build",
    "optipng": "optipng media/*png",
    "clear": "docusaurus clear",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids"
  },
  "dependencies": {
    "@docusaurus/core": "^3.9.1",
    "@docusaurus/plugin-client-redirects": "^3.9.1",
    "@docusaurus/plugin-pwa": "^3.9.1",
    "@docusaurus/preset-classic": "^3.9.1",
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@mdx-js/react": "^3.0.0",
    "@mui/icons-material": "^6.4.8",
    "@mui/material": "^6.4.8",
    "@mui/styles": "^6.4.8",
    "clsx": "^2.1.0",
    "docusaurus-lunr-search": "^3.6.0",
    "docusaurus-pdf": "^1.2.0",
    "docusaurus-plugin-llms": "^0.2.2",
    "mr-pdf": "^1.0.7",
    "prism-react-renderer": "^2.3.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "^3.7.0",
    "@docusaurus/types": "^3.7.0"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 3 chrome version",
      "last 3 firefox version",
      "last 5 safari version"
    ]
  },
  "engines": {
    "node": ">=18.0"
  }
}
